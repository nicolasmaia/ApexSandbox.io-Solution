public void insertAtStart(String cityName, List<String> cities) {
    // Shift elements to the right to make space for the new element
    for (Integer i = cities.size() - 1; i >= 0; i--) {
        cities[i + 1] = cities[i];
    }
    
    // Insert the new element at the beginning
    cities[0] = cityName;
}

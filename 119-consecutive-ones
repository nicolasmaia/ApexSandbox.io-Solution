public Integer maxConsecutiveOnes(Integer[] numbers) {

    if(numbers <> null){
           Integer consecutiveNumber = 0;
            
            for(Integer currentNumber : numbers){
                if(currentNumber == 1){
                    consecutiveNumber += 1;
                } else{
                    consecutiveNumber = 0;
                }
            } 
            return consecutiveNumber;
    }
    return null;

}
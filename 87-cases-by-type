public Map<String, List<Case>> casesByType(List<Case> cases) {

    Map<String, List<Case>> caseMap = new Map<String, List<Case>>();

    if(cases == null){}
    else{
        for (Case currentCase : cases){
            if(currentCase.Type != null){
                caseMap.put(currentCase.Type, cases);
            }
        }
    }
    return caseMap;
}
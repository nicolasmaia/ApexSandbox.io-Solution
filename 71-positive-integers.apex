public List<Integer> positiveIntegers(List<Integer> numbers) {

    if(numbers <> null){

        List<Integer> positiveIntList = new List<Integer>();

        for(Integer currentNumber : numbers){
            if(currentNumber > 0){
                positiveIntList.add(currentNumber);
            } 
            
        }      
        return positiveIntList;   

    } return null;

}
public List<String> orgNames(List<String> adjectives, List<String> animals) {
    if(adjectives <> null || adjectives.size() > 0) && (animals <> null || animals.size() > 0)){
        List<String> orgNameComboList = new List<String>();

        for(String currentAdjective : adjectives){
            for(String currentAnimal : animals){
                orgNameComboList.add(currentAdjective+'-'+currentAnimal);
            }
        }

    }
    return orgNameComboList;
}
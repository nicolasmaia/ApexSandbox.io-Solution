public Set<Id> accountIds(List<Opportunity> opps) {
    Set<Id> idsOfRelatedAccounts = new Set<Id>();
    if(opps <> null){
        for(List<Opportunity> currentOpp : opps){
            idsOfRelatedAccounts.add(currentOpp.AccountId);
        }
    }
    else{
        //do nothing if opps is null
    }
}